    "antd": "^5.27.4",
    "daypilot-pro-react": "https://npm.daypilot.org/daypilot-pro-react/trial/2025.3.6646.tar.gz",
    "lucide": "^0.544.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
